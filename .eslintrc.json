{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb"
        //"plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "import/extensions": ["error", "ignorePackages", {
            "js": "always",
            "jsx": "always"
        }]
    }
}
