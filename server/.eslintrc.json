{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb"
        // "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-underscore-dangle": "off",
        "import/extensions": ["error", "ignorePackages", {
            "js": "always",
            "jsx": "always"
        }],
        "camelcase": "off",
        "no-shadow":"off",
        "operator-linebreak":"off"
    }
}
